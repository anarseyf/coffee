{
  "name": "geo-test",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "backfill:tweets": "cd tweets && BEARER=$(cat bearer.txt) node -r esm backfill.js",
    "update:tweets": "cd tweets && BEARER=$(cat bearer.txt) node -r esm update.js",
    "populate:tweets": "cd tweets && node -r esm populate.js",
    "resolve:tweets": "cd tweets && node -r esm resolve.js",
    "nhoods:tweets": "cd tweets && node -r esm nhoods.js",
    "split:tweets": "cd tweets && node -r esm split.js",
    "nobackfill:tweets": "concurrently \"npm run update:tweets\" \"npm run populate:tweets\" \"npm run resolve:tweets\" \"npm run nhoods:tweets\" \"npm run split:tweets\"",
    "all:tweets": "concurrently \"npm run nobackfill:tweets\" \"npm run backfill:tweets\"",
    "backfill:official": "cd official && node -r esm scrape.js",
    "combine:official": "cd official && node -r esm combine.js",
    "resolve:official": "cd official && node -r esm resolve.js",
    "nhoods:official": "cd official && node -r esm nhoods.js",
    "split:official": "cd official && node -r esm split.js",
    "nobackfill:official": "concurrently \"npm run combine:official\" \"npm run resolve:official\" \"npm run nhoods:official\" \"npm run split:official\"",
    "all:official": "concurrently \"npm run nobackfill:official\" \"npm run backfill:official\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.10.1",
    "axios": "^0.19.2",
    "jsdom": "^16.2.2",
    "lockfile": "^1.0.4",
    "node-fetch": "^2.6.0",
    "node-geocoder": "^3.27.0",
    "semver": "^7.3.2"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "esm": "^3.2.25",
    "nodemon": "^2.0.4"
  },
  "engines": {
    "node": "14.x",
    "npm": "6.x"
  }
}
