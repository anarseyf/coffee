    "prettier": "^2.0.5"
  },
  "scripts": {
    "start": "npm run server:prod",
    "app:dev": "concurrently \"npm run server:dev\" \"npm run client\" \"npm run background:dev\"",
    "client": "cd client && npm run start",
    "server:prod": "NODE_ENV=production node -r esm server.js",
    "server:dev": "NODE_ENV=development nodemon -r esm --config nodemon.json server.js",
    "background:dev": "cd scripts/dispatch && npm run nobackfill:dev",
    "build": "cd client && npm run build",
    "format": "prettier \"./client/src/**/*.{js,jsx,ts,tsx}\" --write",
